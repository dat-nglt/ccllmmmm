<table class="table">
  <thead>
    <tr>
      <th scope="col">STT</th>
      <th scope="col">Tên sản phẩm</th>
      <th scope="col">Giá sản phẩm</th>
      <th scope="col">Tùy chọn</th>
    </tr>
  </thead>
  <tbody>
    <% data.dataProducts.forEach((product, index) => { %>
    <tr>
      <th scope="row"><%= index + 1 %></th>
      <!-- Hiển thị số thứ tự -->
      <td><%= product.productname %></td>
      <!-- Hiển thị tên sản phẩm -->
      <td><%= product.price %></td>
      <!-- Hiển thị giá sản phẩm -->
      <td>
        <a href="http://localhost:3000/chitiet/<%= product.id %>"
          >Xem chi tiết</a
        >
      </td>
      <!-- Link chi tiết sản phẩm -->
    </tr>
    <% }); %>
  </tbody>
</table>
